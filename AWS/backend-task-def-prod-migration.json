{
  "family": "backend-prod-migration",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::354918390375:role/prod-ecs-task-exec-role",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "354918390375.dkr.ecr.ap-northeast-1.amazonaws.com/prod-backend:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/prod-api",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        { "name": "PORT", "value": "5050" },
        { "name": "AWS_ACCOUNT_ID", "value": "354918390375" }
      ],
      "secrets": [
        { "name": "DATABASE_URL", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:DATABASE_URL::" },
        { "name": "ENVIRONMENT", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:ENVIRONMENT::" },
        { "name": "OPENAI_API_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:OPENAI_API_KEY::" },
        { "name": "SECRET_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:SECRET_KEY::" },
        { "name": "STRIPE_SECRET_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:STRIPE_SECRET_KEY::" },
        { "name": "STRIPE_PUBLISHABLE_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:STRIPE_PUBLISHABLE_KEY::" },
        { "name": "STRIPE_WEBHOOK_SECRET", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:STRIPE_WEBHOOK_SECRET::" },
        { "name": "NEXTAUTH_URL", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:NEXTAUTH_URL::" },
        { "name": "AUTH_SECRET", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:AUTH_SECRET::" },
        { "name": "AWS_S3_ICON_BUCKET_NAME", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:354918390375:secret:prod/api/env-bf58bb25-HBWJUD:AWS_S3_ICON_BUCKET_NAME::" }
      ]
    }
  ]
} 